{
  "name": "fiap-post-blog",
  "version": "1.0.7",
  "description": "NestJs Project - Fiap Post-Tech",
  "author": "Kainan Guerra (https://www.linkedin.com/in/kainan-guerra)",
  "contributors": [
    {
      "name": "Gabriel Alves de Melo",
      "url": "https://www.linkedin.com/in/gabriel-melo-94b51b1a2"
    }
  ],
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean:dist": "rimraf dist/*",
    "prebuild": "yarn clean:dist",
    "db:redis:quotation": "yarn ts-node scripts/populate-quotation-redis.ts",
    "db:redis:exchange": "yarn ts-node scripts/populate-exchange-rate-redis.ts",
    "predev": "yarn db:redis:quotation && yarn db:redis:exchange && yarn clean:dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "ts-node src/index.ts",
    "dev": "nest start --tsc --watch",
    "start:dev": "nest start --watch",
    "start:dev:worker": "TZ=UTC nest start --config nest-cli-worker.json --tsc --watch",
    "db:migrate:prod": "typeorm -d dist/src/config/ormconfig-ci.js migration:run",
    "start:prod": "node dist/src/main.js",
    "debug": "nest start --debug 0.0.0.0:9229 --watch",
    "lint": "yarn eslint \"{src,apps,libs,specs}/**/*.ts\" --quiet --cache --cache-strategy content",
    "lint:fix": "yarn lint --fix",
    "test": "env-cmd -f .env.test jest --forceExit --logHeapUsage --noStackTrace",
    "test:v": "yarn test --verbose",
    "test:d": "yarn env-cmd -f .env.test node --inspect-brk=0.0.0.0:9229 --expose-gc --no-compilation-cache ./node_modules/.bin/jest --detectOpenHandles --forceExit --logHeapUsage --runInBand --verbose",
    "test:l": "yarn env-cmd -f .env.test jest --detectOpenHandles --detectLeaks",
    "test:w": "yarn test --watch",
    "test:c": "yarn test --coverage --coverageReporters=lcov",
    "test:u": "yarn test -c jest-unit-config.js",
    "test:f": "yarn test --detectOpenHandles --runInBand -c jest-feature-config.js",
    "release": "yarn semantic-release",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "db:schema-log": "yarn run typeorm -d ./ormconfig.ts schema:log",
    "db:schema-sync": "yarn run typeorm -d ./ormconfig.ts schema:sync",
    "db:create": "ts-node -r tsconfig-paths/register ./scripts/create-schema.ts",
    "db:create-database": "ts-node ./scripts/create-database.ts",
    "db:create-schema": "ts-node ./scripts/create-schema.ts",
    "db:spawn": "yarn db:create-database && yarn db:create-schema && yarn db:migrate",
    "db:drop": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm schema:drop -d ./ormconfig.ts",
    "db:migrate": "yarn run typeorm -d ./ormconfig.ts migration:run",
    "db:migration:generate": "yarn run typeorm -d ./ormconfig.ts migration:generate",
    "db:migration:create": "yarn run typeorm migration:create",
    "db:migration:revert": "yarn run typeorm -d ./ormconfig.ts migration:revert",
    "db:seed": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm-extension seed:run -d ./ormconfig.ts",
    "db:clear": "ts-node -r tsconfig-paths/register ./src/db/scripts/clear-tables.ts",
    "type:check": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@ihelpee/javascript-state-machine": "^1.0.2",
    "@nestjs/axios": "~4.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "~3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "~11.0.0",
    "@types/supertest": "^6.0.3",
    "bcrypt": "~6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.0",
    "fastify": "^5.4.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "supertest": "^7.1.4",
    "typeorm": "^0.3.24",
    "typeorm-extension": "~3.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "9.8.0",
    "@nestjs/cli": "~11.0.5",
    "@nestjs/testing": "~11.0.12",
    "@types/bcrypt": "5.0.2",
    "@types/crypto-js": "4.2.2",
    "@types/dinero.js": "1.9.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/lodash": "~4.17.14",
    "@types/morgan": "1.9.9",
    "@types/multer": "1.4.12",
    "@types/node": "~22.10.5",
    "@types/pg": "8.11.10",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "~8.19.0",
    "@typescript-eslint/parser": "~8.19.0",
    "commitizen": "4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "database-cleaner": "1.3.0",
    "env-cmd": "10.1.0",
    "eslint": "~9.17.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-check-file": "~2.8.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-helpers": "~2.0.1",
    "eslint-plugin-jest": "~28.10.0",
    "eslint-plugin-prettier": "~5.2.1",
    "eslint-plugin-unused-imports": "~4.1.4",
    "globals": "^16.2.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-extended": "5.0.3",
    "lint-staged": "16.0.0",
    "nock": "14.0.4",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "~9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "ulid": "3.0.0",
    "use-yarn": "~2.4.0",
    "wait-for-expect": "3.0.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
